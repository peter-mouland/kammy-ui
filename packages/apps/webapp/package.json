    "@kammy-ui/league-one-table": "0.0.17",
    "@kammy-ui/transfers-page": "0.0.17",
    "react-hot-loader": "3.0.0-beta.6",
    "@kammy-ui/html": "0.0.17",
    "@kammy-ui/fetch-google-sheets": "0.0.18",
    "@kammy-ui/fetch-sky-sports": "0.0.18",
    "@kammy-ui/graphql": "0.0.18",
    "@kammy-ui/koa-middleware-handler-error": "0.0.17",
