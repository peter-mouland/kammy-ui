{
  "name": "@kammy-ui/webapp",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "scripts": {
    "build": "rm -rf compiled && npm run build:server && npm run build:app",
    "build:app": "NODE_ENV=production webpack --config webpack.config.prod.js -p",
    "build:server": "NODE_ENV=production babel src -d compiled",
    "start:dev": "NODE_ENV=development DEBUG=kammy,kammy:* webpack-dev-server --config webpack.config.dev-server.js",
    "start:prod": "NODE_ENV=production node compiled/server-entry.js"
  },
  "dependencies": {
    "@kammy-ui/app-root": "0.0.17",
    "@kammy-ui/html": "0.0.17",
    "@kammy-ui/map-webpack-assets": "0.0.17",
    "@kammy-ui/koa-middleware-handler-error": "0.0.17",
    "@kammy-ui/koa-middleware-headers": "0.0.17",
    "@kammy-ui/koa-middleware-logger": "0.0.17",
    "@kammy-ui/koa-middleware-react": "0.0.17",
    "@kammy-ui/koa-middleware-response-time": "0.0.17",
    "@kammy-ui/polyfills": "0.0.17",
    "@kammy-ui/rules-page": "0.0.17",
    "@kammy-ui/players-page": "0.0.17",
    "@kammy-ui/vendor": "0.0.17",
    "koa": "2.5.1",
    "koa-compress": "3.0.0",
    "koa-convert": "1.2.0",
    "koa-session": "5.8.1",
    "koa-static": "5.0.0",
    "koa-passport": "4.1.0",
    "koa-qs": "2.0.0",
    "koa-router": "7.4.0",
    "prop-types": "15.6.2",
    "redux": "3.7.2",
    "react-router-redux": "4.0.8"
  },
  "devDependencies": {
    "nodemon": "^1.18.2",
    "webpack-merge": "^4.1.3"
  }
}
